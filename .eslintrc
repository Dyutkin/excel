{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc" : "off",
    "object-curly-spacing": [2, "always"],
     "quotes" : "off",
     "no-trailing-spaces": ["error", { "skipBlankLines": true }],
     "indent": [2, "tab"],
     "no-tabs": 0
  }
}